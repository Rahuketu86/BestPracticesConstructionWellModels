%% Beamer Color Theme: Kuala Lumpur
%% To be used with KualaLumpur.sty
%% Created by Alfonso R Reyes. 29 November 2018

\definecolor{DarkGrey}{RGB}{100,100,100}
\definecolor{DarkYellow}{RGB}{230, 172, 0}
\definecolor{grey}{RGB}{191,191,191}
\definecolor{LightGrey}{RGB}{231,231,231}
\definecolor{Orange}{RGB}{204, 89, 0}
\definecolor{KualaGreen}{RGB}{45, 187, 173}
\definecolor{KualaGreenLight}{RGB}{116, 210, 201}
\definecolor{KualaGreenDark}{RGB}{0, 173, 155}
\definecolor{KualaGreenDark}{RGB}{0, 192, 155}
\definecolor{KualaSky}{RGB}{198, 239, 239}
\definecolor{KualaNight}{RGB}{0, 128, 128}
\definecolor{KualaWine}{RGB}{192, 0, 0}

\setbeamercolor{normal text}{fg=black,bg=black!2}
\setbeamercolor{alerted text}{fg=KualaWine}
\setbeamercolor{block body}{fg=black,bg=black!15!white}
\setbeamercolor{block title}{fg=white,bg=KualaGreen}
\setbeamercolor{block body alerted}{fg=KualaNight,bg=KualaSky}
\setbeamercolor{block title alerted}{fg=white,bg=black}
\setbeamercolor{description item}{fg=KualaGreenDark}
\setbeamercolor{enumerate item}{fg=KualaGreenDark,bg=LightGrey}
% change blue to green
\setbeamercolor{enumerate subitem}{fg=KualaGreenDark!50!KualaGreen,bg=DarkGrey}
\setbeamercolor{frametitle}{bg=KualaGreen,fg=white}
\setbeamercolor{itemize item}{fg=KualaGreenDark}
\setbeamercolor{itemize subitem}{fg=KualaGreenDark!50!green}
\setbeamercolor{itemize}{fg=black}
\setbeamercolor{progress bar}{fg=KualaGreenDark}
\setbeamercolor{section}{fg=KualaGreen}
\setbeamercolor{structure}{fg=KualaGreen}
\setbeamercolor{subsection}{fg=KualaGreen}
\setbeamercolor{title}{fg=white,bg=KualaGreen}
\usebeamercolor[fg]{normal text}

